# LLM Provider
LLM_PROVIDER=qwen

# Qwen / Dashscope Configuration
DASHSCOPE_API_KEY=your_dashscope_api_key_here
QWEN_API_BASE=https://dashscope.aliyuncs.com/compatible-mode/v1

# OpenAI Configuration (optional)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_BASE=https://api.openai.com/v1

# Model Configuration
LITELLM_MODEL=qwen/qwen-max
ORCA_ORCHESTRATOR_MODEL=qwen/qwen-max
ORCA_SUBAGENT_MODEL=qwen/qwen-plus
ORCA_ORCHESTRATOR_TEMPERATURE=0.1
ORCA_SUBAGENT_TEMPERATURE=0.1

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_MAX_CONNECTIONS=50

# Application Configuration
LOG_LEVEL=INFO
MAX_TURNS=50
MAX_SUBAGENT_TURNS=20
WORKSPACE_DIR=./workspace

# API Configuration
API_TITLE=Multi-Agent Coding Assistant
API_VERSION=1.0.0
API_PREFIX=/api/v1

# Execution Configuration
COMMAND_TIMEOUT=300
MAX_ENV_RESPONSE_CHARS=12000